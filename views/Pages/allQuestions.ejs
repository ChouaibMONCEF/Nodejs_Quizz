  <body id="gradient">
    <div class="container">
      <div class="qtitle">
        <h1>All Questions</h1>
      </div>
      <table>
        <tr>
          <th>id</th>
          <th>question</th>
          <th>Subject</th>
          <th>level</th>
          <th>points</th>
        </tr>
        <% for(var i=0; i< recordset.length; i++) {%>
        <tr>
          <td><%=recordset[i].id%></td>
          <td><%=recordset[i].question%></td>
          <% if(recordset[i].subjectid == 1){%>
          <td>math</td>
          <% }else if(recordset[i].subjectid == 2){ %>
          <td>Physics</td>
          <% }else { %>
          <td>SVT</td>
          <% } %>
          <td><%=recordset[i].level%></td>
          <td><%=recordset[i].points%></td>

        </tr>
        <% } %>
